<tal:menu define="menuitems viewlet.menus" condition="menuitems">
  <ul tal:attributes="id viewlet.id" class="pull-right">
    <li tal:repeat="menu menuitems">
    <a class="btn" href="" tal:attributes="href menu.action">
      <span src="" title="" alt=""
      tal:attributes="class menu.icon; title menu.title; alt menu.title" />
    </a>
    </li>
  </ul>
</tal:menu>
